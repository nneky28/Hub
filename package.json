    "assembleDebug:assemble": "cd android && ./gradlew assembleDebug",
    "android_staging": "appcenter codepush release-react -a Torilo/myedge-android -d Staging",
    "android_prod": "appcenter codepush release-react -a Torilo/myedge-android -d Production",
    "ios_staging": "appcenter codepush release-react -a Torilo/myedge-ios -d Staging",
